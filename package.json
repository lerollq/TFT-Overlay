{
  "name": "teamfight-tactics-overlay",
  "author": {
    "name": "Quentin Le Rolland"
  },
  "version": "0.1.0",
  "private": true,
  "build": {
    "files": [
      "**/*",
      "!src/",
      "!tsconfig.json",
      "!node_modules/",
      "node_modules/electron-is-dev",
      "!dist/",
      "!public"
    ],
    "nsis": {
      "createDesktopShortcut": "always"
    },
    "appId": "com.electron.teamfight-tactics-overlay",
    "productName": "teamfight-tactics-overlay",
    "copyright": "Copyright Â© 2019 Quentin Le Rolland"
  },
  "dependencies": {
    "@types/node": "^12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-tooltip": "^3.9.3",
    "electron-is-dev": "^1.1.0",
    "electron-store": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-scripts": "^3.0.1",
    "react-tooltip": "^3.10.0",
    "redux": "^4.0.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "pack": "electron-builder --dir",
    "dist": "yarn build && electron-builder build --win --x64",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "typescript": "^3.5.3",
    "wait-on": "^3.2.0"
  }
}
